{
  "name": "passive-osint-suite (light)",
  "image": "mcr.microsoft.com/devcontainers/python:1-3.10",
  "postCreateCommand": "pip install --no-cache-dir -r requirements.txt",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python"
      ]
    }
  },
  "remoteUser": "vscode",
  "shutdownAction": "stopContainer"
}